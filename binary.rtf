def bubblesort(lst):\
    for i in range(len(lst)-1):\
        for j in range(len(lst)-1):\
            if (lst[j]>lst[j+1]):\
                t=lst[j]\
                lst[j]=lst[j+1]\
                lst[j+1]=t\
    return lst\
