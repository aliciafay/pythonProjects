def binarySearch(lst,key):\
    low=0\
    high=len(lst)\
    while high>=low:\
        mid=(low+high)//2\
        if key<lst[mid]:\
            high=mid-1\
        elif key==lst[mid]:\
            return 'Found'\
        else:\
            low=mid+1\
    return 'Not Found'\
